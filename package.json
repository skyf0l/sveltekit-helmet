{
  "name": "sveltekit-helmet",
  "author": "Skyf0l",
  "description": "Security header middleware collection for SvelteKit",
  "license": "MIT",
  "version": "2.1.0",
  "main": "./lib/index.js",
  "type": "module",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "lint": "eslint . && prettier --check .",
    "format": "prettier --write .",
    "test:watch": "jest --watch",
    "test": "jest --coverage",
    "test:e2e": "pnpm --filter e2e test:e2e",
    "test:e2e:install": "pnpm --filter e2e test:e2e:install",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "sveltekit",
    "helmet",
    "security",
    "headers",
    "x-frame-options",
    "csp",
    "hsts",
    "clickjack"
  ],
  "repository": {
    "url": "https://github.com/skyf0l/sveltekit-helmet"
  },
  "dependencies": {
    "helmet": "^8.1.0"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.39.2",
    "@sveltejs/kit": "^2.49.3",
    "@types/jest": "^30.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.0.0",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  },
  "packageManager": "pnpm@10.27.0"
}
