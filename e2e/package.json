  "dependencies": {
    "sveltekit-helmet": "workspace:*"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/kit": "^2.27.0",
    "@sveltejs/vite-plugin-svelte": "^6.1.0",
    "svelte": "^5.37.3",
    "svelte-check": "^4.3.1",
    "typescript": "^5.9.2",
    "vite": "^7.0.6"
  }
}
