    "@sveltejs/kit": "^2.22.2",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "svelte": "^5.35.4",
    "svelte-check": "^4.2.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.2"
  }
}
